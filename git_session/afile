class Human:
	
	def __init__(self, name, age, gender):
		self.name = name
		self.age = age
		self.gender = gender


	def get_name(self):
		if name == "":
			print("name can be an empty string.")
			while True:
				input("please re enter your name:")
			return self.name

	def get_age(self):
		return self.age

	def get_gender(self):
		return self.gender

	def get_complexion(self, complexion):
		complexions = ["fair", "dark", "choccolate"]
		if self.complexion not in complexions:
			print("invalid input for human complexions")
			while True:
				self.complexion = input("enter complexion from the list provided")
			return self.complexion


